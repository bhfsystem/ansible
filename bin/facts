#!/usr/bin/env bash

function main {
  local shome="$(cd -P -- "${BASH_SOURCE%/*}/.." && pwd -P)"
  source "$shome/script/profile"
  
  ansible localhost -c local -m setup -o | sed 's#^[^{]*##' | jq '.ansible_facts as $f | reduce ($f | keys[] | select(startswith("ansible_"))) as $k ({}; .[$k | ltrimstr("ansible_")] = $f[$k])' | jq "$@"
}

source sub "$BASH_SOURCE" "$@"
